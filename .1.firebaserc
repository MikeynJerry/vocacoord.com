{
  "projects": {
    "default": "vocacoord",
    "master": "vocacoord",
    "prod": "vocacoord"
  },
  "ci": {
    "copyVersion": true,
    "createConfig": {
      "master": {
        "env": "staging",
        "firebase": {
          "apiKey": "${STAGE_FIREBASE_API_KEY}",
          "authDomain": "vocacoord.firebaseapp.com",
          "databaseURL": "https://vocacoord.firebaseio.com",
          "projectId": "vocacoord",
          "storageBucket": "vocacoord.appspot.com",
          "messagingSenderId": "317432010698"
        },
        "sentryDsn": "${SENTRY_DSN}",
        "analyticsTrackingId": "${GA_TRACKING_ID}",
        "publicVapidKey": "${STAGE_PUBLIC_VAPID_KEY}"
      },
      "prod": {
        "env": "production",
        "firebase": {
          "apiKey": "${PROD_FIREBASE_API_KEY}",
          "authDomain": "vocacoord.firebaseapp.com",
          "databaseURL": "https://vocacoord.firebaseio.com",
          "projectId": "vocacoord",
          "storageBucket": "vocacoord.appspot.com",
          "messagingSenderId": "317432010698"
        },
        "sentryDsn": "${SENTRY_DSN}",
        "analyticsTrackingId": "${GA_TRACKING_ID}",
        "publicVapidKey": "${PROD_PUBLIC_VAPID_KEY}"
      }
    }
  }
}
